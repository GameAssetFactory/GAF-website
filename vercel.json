{
  "functions": {
      "api/index.php": {
          "runtime": "vercel-php@0.7.1"
      }
  },
  "routes": [
    { "src": "/", "dest": "/api/index.php" },
    { "src": "/(.*\\.php$)", "dest": "/api/$1" }
  ],
  "headers": [
    {
      "source": "/(.*\\.php$)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    }
  ]
}